@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace CollegeWebsite2.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="CollegeWebsite2.styles.css" rel="stylesheet" />
    <link rel="icon" type="image/png" href="favicon.png"/>
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
    <script type="text/javascript" src="/import/popper.min.js"></script>
    <link rel="stylesheet" href="/stylesheet/bootstrap.min.css">
    <script type="text/javascript" src="/import/bootstrap.min.js"></script>
    <script type="text/javascript" src="/import/jquery.js"></script>
    <link rel="stylesheet" href="/stylesheet/datatables.min.css">
    <script type="text/javascript" src="/import/datatables.min.js"></script>
    <script type="text/javascript" src="/import/footable.min.js"></script>
    <script type="text/javascript" src="/script/table.js"></script>
    <script type="text/javascript" src="/script/shared.js"></script>
    <link rel="stylesheet" href="stylesheet/style.css" />
</head>
<body>
    <component type="typeof(App)" render-mode="Server" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
